<launch>

  <arg name="file" default="urban_campus_small" />
  <arg name="speed" default="1" />
  <arg name="play_from" default="0.0" />

  <!-- Sensor Transform configuration -->
  <!-- <include file="$(dirname)/robot/config_outdoor.launch" if="$(arg use_outdoor_config)" /> -->
  <!-- <include file="$(dirname)/robot/config_indoor.launch" unless="$(arg use_outdoor_config)" /> -->

  <include file="$(find ku_dataset_ros)/rosbag/$(arg file).launch" >
    <arg name="speed" value="$(arg speed)" />
    <arg name="play_from" value="$(arg play_from)" />
  </include>

  <!-- rviz -->
  <node pkg="rviz" type="rviz" name="$(arg file)_rviz" output="screen"
    args="-d $(find ku_dataset_ros)/launch/rviz/$(arg file).rviz" />
</launch>